<section class='main-content'>

    <div class='form-titulo'>Editar objetivo</div>

    <%= form_for(@objetivo) do |form| %>
        <p class='form-label'><%= form.label :titulo %></p>
        <p class='form-input'><%= form.text_field :titulo %></p>
        
        <p class='form-label'><%= form.label :descricao %></p>
        <p class='form-input'><%= form.text_field :descricao %></p>

        <p class='form-btn'><%= form.submit 'Atualizar Objetivo' %></p>
    <% end %>

</section>